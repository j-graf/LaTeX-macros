%% item numbering  %%
\renewcommand{\theequation}{\arabic{section}.\arabic{equation}}
\renewcommand{\thefigure}{\arabic{section}.\arabic{figure}}
\renewcommand{\thetable}{\arabic{section}.\arabic{table}}

\numberwithin{equation}{section}
\numberwithin{figure}{section}
\numberwithin{table}{section}

%% Number items consecutively within sections, as in Definition 2.1,
%% Lemma 2.2, Theorem 2.3, Corollary 2.4, Remark 2.5

%% These environments are set in italic type
\newtheorem{theorem}{Theorem}[section]
\newtheorem{corollary}[theorem]{Corollary}
\newtheorem{proposition}[theorem]{Proposition}
\newtheorem{lemma}[theorem]{Lemma}
\newtheorem{conjecture}[theorem]{Conjecture}

\newtheorem*{theorem*}{Theorem}

%% These environments are set in Roman (plain) type
\theoremstyle{remark}
\newtheorem{remark}[theorem]{Remark}
\theoremstyle{definition}
\newtheorem{definition}[theorem]{Definition}
\newtheorem{example}[theorem]{Example}
\newtheorem{notation}[theorem]{Notation}
\newtheorem{recall}[theorem]{Recall}


\newtheorem*{definition*}{Definition}

%% Don't number \subsubsections
\renewcommand{\thesubsubsection}{\hskip -1em}

